  body
    .intro
      .intro__inner
        .container-xl
          .row
            .col-md-6
              .intro__left.h-100
                h1.intro__title Food
                h3.intro__subtitle Discover Restaurant<br> & Delicious Food
                .intro__input-wrapper.input-wrapper
                  input.input-wrapper__input(type="text" placeholder="search restaurant, food")
                  input.input-wrapper__btn(type="submit" value="go")  
            .col-md-6
              .intro__right.h-100
                .intro__slider
                  .intro__slider-wrapper
                    img.intro__slider-bg(src="./img/slider-bg.webp" alt="")
                    .intro__slider-item
                      img.intro__slider-lettuce(src="./img/slider-lettuce.webp" alt="")
                      img.intro__slider-img(src="img/slider-img.webp" alt="")
      a.intro__btn(href="#!")
        i.fas.fa-map-marker-alt
        span Rajshahi
      svg.intro__slider-arrow.intro__slider-left-arrow
        use(xlink:href="img/svg/sprite.svg#arrow-left")
      svg.intro__slider-arrow.intro__slider-right-arrow
        use(xlink:href="img/svg/sprite.svg#arrow-right")
      .intro__aside

